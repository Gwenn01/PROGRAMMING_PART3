import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a num1: ");
		int num1 = s.nextInt();
		System.out.print("Enter a num2: ");
		int num2 = s.nextInt();
		
		int i = 1;

		while(i <= num2){
			num1++;
			i++;
		}
		System.out.print("The sum is " + num1);
	}
}