import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s =  new Scanner(System.in);
		System.out.print("Enter a numbers: ");
		int n = s.nextInt();
		int rem, result = 0;
		
		while(n != 0){
		 rem = n % 10;
		 result = result + rem;	
		 n/= 10;	
		}
		System.out.print("The sum of given numbers is " + result);
	}
}