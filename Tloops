public class Main {
	public static void main(String[] args) {
		
		for(int i = 1; i <= 9; i++){
		    if(i == 1){
		        for(int j = 1; j <= 9; j++){
		            System.out.print("*");
		        }
		    }else{
		        for(int j = 1; j <= 9; j++){
		            if(j == 5){
		            System.out.print("*");
		            }else{
		                System.out.print(" ");
		            }
		        }
		    }
		    System.out.println();
		}
		
	}
}